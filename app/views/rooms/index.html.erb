<h1>Listing rooms</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Max head count</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @rooms.each do |room| %>
  <tr>
    <td><%= room.name %></td>
    <td><%= room.max_head_count %></td>
    <td><%= link_to 'Show', room %></td>
    <td><%= link_to 'Destroy', room, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>
